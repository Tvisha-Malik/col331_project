{
    "env": {},
    "configurations": [
        {
            "name": "Linux",
            "compilerPath": "/usr/bin/gcc",
            "intelliSenseMode": "linux-gcc-x86",
            "includePath": [
                "/usr/include"
            ],
            "compileCommands": "${workspaceFolder}/compile_commands.json",
            "mergeConfigurations": true,
            "browse": {
                "path": [
                    "/usr/include",
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4,
    "enableConfigurationSquiggles": true
}